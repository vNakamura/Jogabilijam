pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- metal jer corgi
-- by vinicius nakamura
palt(11,true)
palt(0,false)

function game_upd()
	if (btnp(4)) show_menu()
	player_upd()
	foreach(actors,actor_upd)
end

function game_drw()
	cls()
	rectfill(0,0,127,127,3)
	foreach(actors,actor_drw)
end

function game_start()
	music(1)
	actors={}
	player=create_actor({
		spr=21,
		h=1,
		x=64,
		y=64
	})
	add(actors,player)
	sushi=create_actor({
		x=10,
		y=30,
		spr=1,
		h=2,
		hasfov=true
	})
	add(actors,sushi)
	scn_upd = game_upd
	scn_drw = game_drw
end

function player_upd()
	if btn(0) then
		player.x-=1
		player.flip=true
	end
	if btn(1) then
		player.x+=1
		player.flip=false
	end
	if(btn(2))player.y-=1
	if(btn(3))player.y+=1
end

function check_fov(a,p)
	if(not a.hasfov) return false
	local dy = abs(p.y-a.y)
	local dx = abs(p.x-a.x)
	if dx<80 and dy<dx*.4 then
	 return true
	end
 return false
end

function menu_upd()
	if btnp(4) then
		sfx(0)
	 transition(game_start)
	end
	menu.count+=1
	if (menu.count > 30) menu.count=0
end
function menu_drw()
	cls()
	rectfill(0,0,127,127,11)
	if menu.count<15 then
		print("aperte z",48,90,0)
	end
	print("@vnaka            #jogabilijam",5,120,0)
	spr(1,45,68,5,2)
end
function show_menu()
	music(0)
	menu = {count = 0}
	scn_upd = menu_upd
	scn_drw = menu_drw
end

function _init()
	-- show_menu()
	game_start()
end
function _update()
	scn_upd()
	t_upd()
end
function _draw()
	scn_drw()
	t_drw()
end

function create_actor(actor)
	local a = {}
	for k,v in pairs(actor) do
		a[k] = v
	end
	return a
end
function actor_upd(a)
	a.in_fov = check_fov(a,player)
end
function actor_drw(a)
	spr(a.spr,a.x-4,a.y+4-a.h*8,1,a.h,a.flip)
	if(a.in_fov) spr(16,a.x-4,a.y-6-a.h*8)
end

trst = {} --transition
function t_drw ()
	if(not trst.on) return
	if trst.c < 4 then
 	for i=0,31 do
 		local s=i*4+trst.c
	 	line(0,s,s,0,0)
		line(s+1,127,127,s+1,0)
	 end
	else
	 local s=max(trst.c-8,0)*8
	 rectfill(0,0,127,64-s,0)
	 rectfill(0,64+s,127,127,0)
	 rectfill(0,0,64-s,127,0)
	 rectfill(64+s,0,127,127,0)
	end
end
function t_upd()
	if(not trst.on) return
	trst.c += 1
	if(trst.c==6)trst.cb()
	if(trst.c>17)trst.on=false
end
function transition(cb)
	scn_upd=nothing
	scn_drw=nothing
	trst.c = 0
	trst.cb = cb
	trst.on = true
end
function nothing() end
