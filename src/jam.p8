function _update()
	scn_upd()
	ctrs_upd()
	t_upd()
end
function _draw()
	scn_drw()
	t_drw()
	print(debug_s,2,2,8)
end

ctrs={} --counters
function delay(...) add(ctrs,{...})end
function ctrs_upd()
 for c in all(ctrs) do
  c[1]-=1
  if c[1]<=0 then
  	c[2](c[3])
  	del(ctrs,c)
  end
 end
end
trst={} --transition
function t_drw ()
	if(not trst.on) return
	if trst.c < 4 then
 	for i=0,31 do
 		local s=i*4+trst.c
	 	line(0,s,s,0,0)
		line(s+1,127,127,s+1,0)
	 end
	else
	 local s=max(trst.c-8,0)*8
	 rectfill(0,0,127,64-s,0)
	 rectfill(0,64+s,127,127,0)
	 rectfill(0,0,64-s,127,0)
	 rectfill(64+s,0,127,127,0)
	end
end
function t_upd()
	if(not trst.on) return
	trst.c += 1
	if(trst.c==6)trst.cb()
	if(trst.c>17)trst.on=false
end
function transition(cb)
	scn_upd=nothing
	scn_drw=nothing
	trst.c = 0
	trst.cb = cb or nothing
	trst.on = true
end
function nothing() end
